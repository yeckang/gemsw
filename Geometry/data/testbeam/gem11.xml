<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../../../../DetectorDescription/Schema/DDLSchema.xsd">

<!-- Modified GEM11.XML file to use to mimick 10x10 chambers used in testbeam -->
<!-- In order not to mess up with the current numbering scheme, we keep 8 eta partitions
     and we keep also all volumes for long and short chambers ... I ll make them identical though
     GEM10x10 are rectangular and have the dimensions of the 10x10 chambers
     Actually the 10x10 chambers are 8.95 x 8.95 cm2 and have 358 strips with 250um pitch -->
<!-- Local Coordinate system: Z=height, X=width, Y=thickness -->
<!-- Always Half-lenghts are specified -->

<ConstantsSection label="gem11.xml" eval="true">
  <!-- ETA PARTITIONS -->
  <Constant name="g10x10" value="4.9750*cm"/> <!-- eta partition heights -->
  <Constant name="dzS1L" value="[g10x10]"/> <!-- eta partition heights -->
  <Constant name="dzS2L" value="[g10x10]"/>
  <Constant name="dzS3L" value="[g10x10]"/>
  <Constant name="dzS4L" value="[g10x10]"/>
  <Constant name="dzS5L" value="[g10x10]"/>
  <Constant name="dzS6L" value="[g10x10]"/>
  <Constant name="dzS7L" value="[g10x10]"/>
  <Constant name="dzS8L" value="[g10x10]"/>
  <Constant name="dzS1S" value="[g10x10]"/>
  <Constant name="dzS2S" value="[g10x10]"/>
  <Constant name="dzS3S" value="[g10x10]"/>
  <Constant name="dzS4S" value="[g10x10]"/>
  <Constant name="dzS5S" value="[g10x10]"/>
  <Constant name="dzS6S" value="[g10x10]"/>
  <Constant name="dzS7S" value="[g10x10]"/>
  <Constant name="dzS8S" value="[g10x10]"/>
  <!-- <Constant name="dzGap" value="4.9750*mm"/> -->
  <Constant name="dzGap" value="0.2500*mm"/>
  <Constant name="dzInL" value="([dzS1L]+[dzS2L]+[dzS3L]+[dzS4L]+[dzS5L]+[dzS6L]+[dzS7L]+[dzS8L]+7*[dzGap])"/> <!-- Total lenght in z (height) Short Chamber -->
  <Constant name="dzInS" value="([dzS1S]+[dzS2S]+[dzS3S]+[dzS4S]+[dzS5S]+[dzS6S]+[dzS7S]+[dzS8S]+7*[dzGap])"/> <!-- Total length in z (height) Long Chamber -->
  <Constant name="dzLS"  value="([dzInL]-[dzInS])"/>                                                           <!-- Difference in Length between Short and Long -->
  <Constant name="z10L"  value="([dzInL]-[dzS1L])"/> 
  <Constant name="z20L"  value="([z10L]-[dzS1L]-2*[dzGap]-[dzS2L])"/> 
  <Constant name="z30L"  value="([z20L]-[dzS2L]-2*[dzGap]-[dzS3L])"/> 
  <Constant name="z40L"  value="([z30L]-[dzS3L]-2*[dzGap]-[dzS4L])"/> 
  <Constant name="z50L"  value="([z40L]-[dzS4L]-2*[dzGap]-[dzS5L])"/> 
  <Constant name="z60L"  value="([z50L]-[dzS5L]-2*[dzGap]-[dzS6L])"/> 
  <Constant name="z70L"  value="([z60L]-[dzS6L]-2*[dzGap]-[dzS7L])"/> 
  <Constant name="z80L"  value="([z70L]-[dzS7L]-2*[dzGap]-[dzS8L])"/> 
  <Constant name="z10S"  value="([dzInS]-[dzS1S])"/> 
  <Constant name="z20S"  value="([z10S]-[dzS1S]-2*[dzGap]-[dzS2S])"/> 
  <Constant name="z30S"  value="([z20S]-[dzS2S]-2*[dzGap]-[dzS3S])"/> 
  <Constant name="z40S"  value="([z30S]-[dzS3S]-2*[dzGap]-[dzS4S])"/> 
  <Constant name="z50S"  value="([z40S]-[dzS4S]-2*[dzGap]-[dzS5S])"/> 
  <Constant name="z60S"  value="([z50S]-[dzS5S]-2*[dzGap]-[dzS6S])"/> 
  <Constant name="z70S"  value="([z60S]-[dzS6S]-2*[dzGap]-[dzS7S])"/> 
  <Constant name="z80S"  value="([z70S]-[dzS7S]-2*[dzGap]-[dzS8S])"/> 
  <Constant name="dxTop" value="[g10x10]"/>   <!-- eta partition width -->
  <!-- <Constant name="Angle" value="0.0001*deg"/>   setting angle to zero makes rectangles -->
  <Constant name="Angle" value="90*deg"/>  <!-- setting angle to zero makes rectangles -->
  <Constant name="slope" value="tan([Angle])"/>
  <Constant name="dx11L" value="([dxTop]-[slope]*([dzInL]-[z10L]+[dzS1L]))"/>
  <Constant name="dx12L" value="([dxTop]-[slope]*([dzInL]-[z10L]-[dzS1L]))"/>
  <Constant name="dx21L" value="([dxTop]-[slope]*([dzInL]-[z20L]+[dzS2L]))"/>
  <Constant name="dx22L" value="([dxTop]-[slope]*([dzInL]-[z20L]-[dzS2L]))"/>
  <Constant name="dx31L" value="([dxTop]-[slope]*([dzInL]-[z30L]+[dzS3L]))"/>
  <Constant name="dx32L" value="([dxTop]-[slope]*([dzInL]-[z30L]-[dzS3L]))"/>
  <Constant name="dx41L" value="([dxTop]-[slope]*([dzInL]-[z40L]+[dzS4L]))"/>
  <Constant name="dx42L" value="([dxTop]-[slope]*([dzInL]-[z40L]-[dzS4L]))"/>
  <Constant name="dx51L" value="([dxTop]-[slope]*([dzInL]-[z50L]+[dzS5L]))"/>
  <Constant name="dx52L" value="([dxTop]-[slope]*([dzInL]-[z50L]-[dzS5L]))"/>
  <Constant name="dx61L" value="([dxTop]-[slope]*([dzInL]-[z60L]+[dzS6L]))"/>
  <Constant name="dx62L" value="([dxTop]-[slope]*([dzInL]-[z60L]-[dzS6L]))"/>
  <Constant name="dx71L" value="([dxTop]-[slope]*([dzInL]-[z70L]+[dzS7L]))"/>
  <Constant name="dx72L" value="([dxTop]-[slope]*([dzInL]-[z70L]-[dzS7L]))"/>
  <Constant name="dx81L" value="([dxTop]-[slope]*([dzInL]-[z80L]+[dzS8L]))"/>
  <Constant name="dx82L" value="([dxTop]-[slope]*([dzInL]-[z80L]-[dzS8L]))"/>
  <Constant name="dx11S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z10S]+[dzS1S]))"/>
  <Constant name="dx12S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z10S]-[dzS1S]))"/>
  <Constant name="dx21S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z20S]+[dzS2S]))"/>
  <Constant name="dx22S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z20S]-[dzS2S]))"/>
  <Constant name="dx31S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z30S]+[dzS3S]))"/>
  <Constant name="dx32S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z30S]-[dzS3S]))"/>
  <Constant name="dx41S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z40S]+[dzS4S]))"/>
  <Constant name="dx42S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z40S]-[dzS4S]))"/>
  <Constant name="dx51S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z50S]+[dzS5S]))"/>
  <Constant name="dx52S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z50S]-[dzS5S]))"/>
  <Constant name="dx61S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z60S]+[dzS6S]))"/>
  <Constant name="dx62S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z60S]-[dzS6S]))"/>
  <Constant name="dx71S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z70S]+[dzS7S]))"/>
  <Constant name="dx72S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z70S]-[dzS7S]))"/>
  <Constant name="dx81S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z80S]+[dzS8S]))"/>
  <Constant name="dx82S" value="([dxTop]-[slope]*([dzInL]+[dzLS]-[z80S]-[dzS8S]))"/>
  <!-- GEM BOX - ALUMINIUM - ... ??? why do we have volume A and volume B ??? -->
  <Constant name="dyGB11"      value="90*cm"/>      <!-- Thickness of the total chamber - No need to change -->
  <Constant name="dzGB11L"     value="40*cm"/>       <!-- Lenght of total chamber - Let's take 2cm border w.r.t. active volume -->
  <Constant name="dzGB11S"     value="([dzGB11L]-[dzLS])"/>
  <Constant name="dxTopGB11L"  value="6.9750*cm"/>     <!-- width of the total chamber - Let's take 2cm borders w.r.t. active volume -->
  <Constant name="dxTopGB11S"  value="([dxTopGB11L]-2*[slope]*[dzLS])"/>
  <Constant name="dxBotGB11"   value="([dxTopGB11L]-2*[slope]*[dzGB11L])"/>
  <Constant name="dzTot"       value="([dxTop]-[dzInL]-[dzGB11L])"/>
  <Constant name="rPosL"       value="([dzTot]+[dzGB11L])"/>
  <Constant name="rPosS"       value="([dzTot]+[dzGB11S])"/>
  <Constant name="dyGA11"      value="9.250*mm"/>      <!-- Thickness of the total chamber - No need to change - but why different from B ? -->
  <Constant name="dzGA11L"     value="90*cm"/>       
  <Constant name="dzGA11S"     value="([dzGA11L]-[dzLS])"/>
  <Constant name="dxBotGA11"   value="12.9500*cm"/>    
  <Constant name="dxTopGA11L"  value="12.9500*cm"/>    
  <Constant name="slope1"      value="(([dxTopGA11L]-[dxBotGA11])/(2*[dzGA11L]))"/>
  <Constant name="dxTopGA11S"  value="([dxTopGA11L]-2*[slope1]*[dzLS])"/>
  <Constant name="dyBase"      value="3.2*mm"/>        <!-- Thickness PCB readout board - for us 1.85mm - but ok not to change -->
  <Constant name="dyTop"       value="1.0*mm"/>
  <Constant name="dxFrame"     value="10.0*mm"/>       <!-- Thickness of Int+Ext frame -->
  <!-- GEM INTERIOR - AIR - => remove frame from GEMBOX -->
  <Constant name="dyGBI11"     value="([dyGB11]-0.5*([dyBase]+[dyTop]))"/>
  <Constant name="dzGBI11L"    value="([dzGB11L]-[dxFrame])"/>
  <Constant name="dzGBI11S"    value="([dzGB11S]-[dxFrame])"/>
  <Constant name="dxTopGBI11L" value="([dxTopGB11L]-[slope]*[dxFrame])"/>
  <Constant name="dxBotGBI11"  value="([dxTopGBI11L]-2*[slope]*[dzGBI11L])"/>
  <Constant name="dxTopGBI11S" value="([dxBotGBI11]+2*[slope]*[dzGBI11S])"/>
  <Constant name="ypGM11"      value="(-[dyGB11]+[dyBase]+[dyGBI11])"/>
  <Constant name="dyGAI11"     value="([dyGA11]-0.5*[dyTop])"/>
  <Constant name="dzGAI11L"    value="([dzGA11L]-[dxFrame])"/>
  <Constant name="dzGAI11S"    value="([dzGA11S]-[dxFrame])"/>
  <Constant name="dxTopGAI11L" value="([dxTopGA11L]-[dyTop])"/>
  <Constant name="dxBotGAI11"  value="([dxTopGAI11L]-2*[slope1]*[dzGAI11L])"/>
  <Constant name="dxTopGAI11S" value="([dxBotGAI11]+2*[slope1]*[dzGAI11S])"/>
  <Constant name="dxTopGBJ11L" value="[dxTopGAI11L]"/>
  <Constant name="dxBotGBJ11"  value="([dxTopGBJ11L]-2*[slope1]*[dzGBI11L])"/>
  <Constant name="dxTopGBJ11S" value="([dxBotGBJ11]+2*[slope1]*[dzGBI11S])"/>
<!-- GEM FOILS -->
  <Constant name="dyFoil"      value="0.025*mm"/>
  <Constant name="dyGap1"      value="(3.0*mm-[dyFoil])/2"/>
  <Constant name="dyGap2"      value="(1.0*mm-2*[dyFoil])/2"/>
  <Constant name="dyGap3"      value="(2.0*mm-2*[dyFoil])/2"/>
  <Constant name="dyGap4"      value="(1.0*mm-[dyFoil])/2"/>
  <Constant name="yGap1"       value="(-[dyGBI11]+[dyGap1])"/>
  <Constant name="yFoil1"      value="([yGap1]+[dyGap1]+[dyFoil])"/>
  <Constant name="yGap2"       value="([yFoil1]+[dyGap2]+[dyFoil])"/>
  <Constant name="yFoil2"      value="([yGap2]+[dyGap2]+[dyFoil])"/>
  <Constant name="yGap3"       value="([yFoil2]+[dyGap3]+[dyFoil])"/>
  <Constant name="yFoil3"      value="([yGap3]+[dyGap3]+[dyFoil])"/>
  <Constant name="yGap4"       value="([yFoil3]+[dyGap4]+[dyFoil])"/>
<!-- GEM Readout Board - FR4 - Covers the frame -->
  <Constant name="dyReadOut"   value="1.5*mm"/>
  <Constant name="yReadOut"    value="(-[dyGBI11]+7.0*mm+[dyReadOut])"/>
  <Constant name="dzCardL"     value="36.800*cm"/>   
  <Constant name="dzCardS"     value="([dzCardL]-[dzLS])"/>
  <Constant name="dxTopCardL"  value="5.975*cm"/>
  <!-- <Constant name="dxBotCard"   value="([dxTopCardL]-2*[slope]*[dzCardL])"/> -->
  <Constant name="dxBotCard"   value="[g10x10]"/>
  <Constant name="dxTopCardS"  value="([dxBotCard]+2*[slope]*[dzCardS])"/>
  <Constant name="dzFoilL"     value="36.800*cm"/>   
  <Constant name="dzFoilS"     value="([dzFoilL]-[dzLS])"/>
  <Constant name="dxTopFoilL"  value="5.975*cm"/> 
  <Constant name="dxBotFoil"   value="([dxTopFoilL]-2*[slope]*[dzFoilL])"/>
  <Constant name="dxTopFoilS"  value="([dxBotFoil]+2*[slope]*[dzFoilS])"/>
  <Constant name="Tilt"        value="[gemf:Tilt]"/> <!-- ???? -->
<!-- Cooling -->
  <Constant name="TubeBotL"    value="60.0*mm"/>
  <Constant name="TubeHeightL" value="1140.0*mm"/>
  <Constant name="TubeHeightS" value="([TubeHeightL]-2*[dzLS])"/>
  <Constant name="TubeMidL"    value="25.0*mm"/>
  <Constant name="TubeTopL"    value="100.0*mm"/>
  <Constant name="TubeRMin"    value="3.0*mm"/>
  <Constant name="TubeRMax"    value="4.0*mm"/>
  <Constant name="TubeSlantLL" value="[TubeHeightL]/cos([Tilt])"/>
  <Constant name="TubeSlantLS" value="[TubeHeightS]/cos([Tilt])"/>
  <Constant name="xTubeBot"    value="0.0*fm"/>
  <Constant name="xTubeSlantL" value="(([TubeBotL]/2)+([TubeRMax]*cos([Tilt]))+([TubeSlantLL]*sin([Tilt])/2))"/>
  <Constant name="xTubeSlantS" value="(([TubeBotL]/2)+([TubeRMax]*cos([Tilt]))+([TubeSlantLS]*sin([Tilt])/2))"/>
  <Constant name="xTubeMidL"   value="([xTubeSlantL]+([TubeRMax]*cos([Tilt]))+([TubeSlantLL]*sin([Tilt])+[TubeMidL])/2)"/>
  <Constant name="xTubeMidS"   value="([xTubeSlantS]+([TubeRMax]*cos([Tilt]))+([TubeSlantLS]*sin([Tilt])+[TubeMidL])/2)"/>
  <Constant name="xTubeTopL"   value="([xTubeMidL]+[TubeMidL]/2+[TubeRMax])"/>
  <Constant name="xTubeTopS"   value="([xTubeMidS]+[TubeMidL]/2+[TubeRMax])"/>
  <Constant name="zTubeTopL"   value="([dzGBI11L]-[TubeTopL]/2)"/>
  <Constant name="zTubeTopS"   value="([dzGBI11S]-[TubeTopL]/2)"/>
  <Constant name="zTubeMidL"   value="([zTubeTopL]-[TubeTopL]/2+[TubeRMax])"/>
  <Constant name="zTubeMidS"   value="([zTubeTopS]-[TubeTopL]/2+[TubeRMax])"/>
  <Constant name="zTubeSlantL" value="([zTubeMidL]+[TubeRMax]-([TubeSlantLL]*cos([Tilt])/2))"/>
  <Constant name="zTubeSlantS" value="([zTubeMidS]+[TubeRMax]-([TubeSlantLS]*cos([Tilt])/2))"/>
  <Constant name="zTubeBotL"   value="([zTubeSlantL]+[TubeRMax]-([TubeSlantLL]*cos([Tilt])/2))"/>
  <Constant name="zTubeBotS"   value="([zTubeSlantS]+[TubeRMax]-([TubeSlantLS]*cos([Tilt])/2))"/>
<!-- Readout Strips -->
  <Constant name="dyStrip"    value="0.0175*mm"/>    <!-- Error in original description ... strips are 35um Cu -->
  <Constant name="dzStrip"    value="4.950*mm"/>     
  <Constant name="dzStripX"   value="7.0*mm"/>
<!-- No Idea ... Need to understand  -->
  <Constant name="Gap01"      value="100.00*mm"/> <!-- 10cm gap ??? -->
  <Constant name="Gap12"      value="99.00*mm"/>
  <Constant name="Gap23"      value="100.00*mm"/>
  <Constant name="Gap34"      value="69.00*mm"/>
  <Constant name="Gap45"      value="100.00*mm"/>
  <Constant name="Gap56"      value="100.00*mm"/>
  <Constant name="Gap67"      value="100.00*mm"/>
  <Constant name="Gap78"      value="100.00*mm"/>
  <Constant name="Gap89"      value="100.00*mm"/>
  <Constant name="yStrip"     value="([yReadOut]+[dyReadOut]+[dyStrip])"/>
  <Constant name="zStrip0L"   value="([dzGBI11L]-86.68*mm)"/> <!-- where does this magic number come from ??? -->
  <Constant name="zStrip1L"   value="([zStrip0L]-[Gap01])"/>
  <Constant name="zStrip2L"   value="([zStrip1L]-[Gap12])"/>
  <Constant name="zStrip3L"   value="([zStrip2L]-[Gap23])"/>
  <Constant name="zStrip4L"   value="([zStrip3L]-[Gap34])"/>
  <Constant name="zStrip5L"   value="([zStrip4L]-[Gap45])"/>
  <Constant name="zStrip6L"   value="([zStrip5L]-[Gap56])"/>
  <Constant name="zStrip7L"   value="([zStrip6L]-[Gap67])"/>
  <Constant name="zStrip8L"   value="([zStrip7L]-[Gap78])"/>
  <Constant name="zStrip9L"   value="([zStrip8L]-[Gap89])"/>
  <Constant name="zStrip0S"   value="([dzGBI11S]-86.68*mm)"/> <!-- where does this magic number come from ??? -->
  <Constant name="zStrip1S"   value="([zStrip0S]-[Gap01])"/>
  <Constant name="zStrip2S"   value="([zStrip1S]-[Gap12])"/>
  <Constant name="zStrip3S"   value="([zStrip2S]-[Gap23])"/>
  <Constant name="zStrip4S"   value="([zStrip3S]-[Gap34])"/>
  <Constant name="zStrip5S"   value="([zStrip4S]-[Gap45])"/>
  <Constant name="zStrip6S"   value="([zStrip5S]-[Gap56])"/>
  <Constant name="zStrip7S"   value="([zStrip6S]-[Gap67])"/>
  <Constant name="zStrip8S"   value="([zStrip7S]-[Gap78])"/>
  <Constant name="zStrip9S"   value="([zStrip8S]-[Gap89])"/>
<!-- Plugin Cards / Hybrids -->
  <Constant name="dxHybrid"   value="25.0*mm"/>
  <Constant name="dyHybrid"   value="0.5*mm"/>
  <Constant name="dzHybrid"   value="22.0*mm"/>
  <Constant name="xHybrid0"   value="107.97*mm"/>
  <Constant name="xHybrid1"   value="102.16*mm"/>
  <Constant name="xHybrid2"   value="96.35*mm"/>
  <Constant name="xHybrid3"   value="90.54*mm"/>
  <Constant name="xHybrid4"   value="84.74*mm"/>
  <Constant name="xHybrid5"   value="84.74*mm"/>
  <Constant name="xHybrid6"   value="78.93*mm"/>
  <Constant name="xHybrid7"   value="73.12*mm"/>
  <Constant name="xHybrid8"   value="67.31*mm"/>
  <Constant name="xHybrid9"   value="61.50*mm"/>
  <Constant name="yHybrid"    value="([yStrip]+[dyStrip]+[dyHybrid])"/>
  <Constant name="yTube"      value="([yHybrid]+[dyHybrid]+[TubeRMax])"/>
</ConstantsSection>

<SolidSection label="gem11.xml">
  <Tubs name="GE11" rMin="1.265*m" rMax="2.64*m" dz="90*cm " startPhi="0*deg" deltaPhi="360*deg"/>

  <Trd1 name="GB11L" dz="[dzGB11L]" dy1="[dyGB11]" dy2="[dyGB11]" dx1="[dxBotGB11]" dx2="[dxTopGB11L]" />
  <Trd1 name="GB11S" dz="[dzGB11S]" dy1="[dyGB11]" dy2="[dyGB11]" dx1="[dxBotGB11]" dx2="[dxTopGB11S]" />
  <Trd1 name="GA11L" dz="[dzGA11L]" dy1="[dyGA11]" dy2="[dyGA11]" dx1="[dxBotGA11]" dx2="[dxTopGA11L]" />
  <Trd1 name="GA11S" dz="[dzGA11S]" dy1="[dyGA11]" dy2="[dyGA11]" dx1="[dxBotGA11]" dx2="[dxTopGA11S]" />
  <UnionSolid name="GEMBox11L">
    <rSolid name="GB11L"/>
    <rSolid name="GA11L"/>
    <Translation x="0.0*fm" y="([dyGB11]+[dyGA11])" z="([dzGB11L]-[dzGA11L])"/>
  </UnionSolid>
  <UnionSolid name="GEMBox11S">
    <rSolid name="GB11S"/>
    <rSolid name="GA11S"/>
    <Translation x="0.0*fm" y="([dyGB11]+[dyGA11])" z="([dzGB11S]-[dzGA11S])"/>
  </UnionSolid>

  <Trd1 name="GBI11L" dz="[dzGBI11L]" dy1="[dyGBI11]" dy2="[dyGBI11]" dx1="[dxBotGBI11]" dx2="[dxTopGBI11L]" />
  <Trd1 name="GBI11S" dz="[dzGBI11S]" dy1="[dyGBI11]" dy2="[dyGBI11]" dx1="[dxBotGBI11]" dx2="[dxTopGBI11S]" />
  <Trd1 name="GBJ11L" dz="[dzGBI11L]" dy1="0.5*[dyTop]" dy2="0.5*[dyTop]" dx1="[dxBotGBJ11]" dx2="[dxTopGBJ11L]" />
  <Trd1 name="GBJ11S" dz="[dzGBI11S]" dy1="0.5*[dyTop]" dy2="0.5*[dyTop]" dx1="[dxBotGBJ11]" dx2="[dxTopGBJ11S]" />
  <UnionSolid name="GEMInterior11L">
    <rSolid name="GBI11L"/>
    <rSolid name="GBJ11L"/>
    <Translation x="0.0*fm" y="([dyGBI11]+0.5*[dyTop])" z="0.0*fm"/>
  </UnionSolid>
  <UnionSolid name="GEMInterior11S">
    <rSolid name="GBI11S"/>
    <rSolid name="GBJ11S"/>
    <Translation x="0.0*fm" y="([dyGBI11]+0.5*[dyTop])" z="0.0*fm"/>
  </UnionSolid>
  <Trd1 name="GAI11L" dz="[dzGAI11L]" dy1="[dyGAI11]" dy2="[dyGAI11]" dx1="[dxBotGAI11]" dx2="[dxTopGAI11L]" />
  <Trd1 name="GAI11S" dz="[dzGAI11S]" dy1="[dyGAI11]" dy2="[dyGAI11]" dx1="[dxBotGAI11]" dx2="[dxTopGAI11S]" />
  <UnionSolid name="GMAX11L">
    <rSolid name="GEMInterior11L"/>
    <rSolid name="GAI11L"/>
    <Translation x="0.0*fm" y="([dyGBI11]+[dyTop]+[dyGAI11])" z="([dzGBI11L]-[dzGAI11L])"/>
  </UnionSolid>
  <UnionSolid name="GMAX11S">
    <rSolid name="GEMInterior11S"/>
    <rSolid name="GAI11S"/>
    <Translation x="0.0*fm" y="([dyGBI11]+[dyTop]+[dyGAI11])" z="([dzGBI11S]-[dzGAI11S])"/>
  </UnionSolid>

